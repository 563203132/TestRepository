@import 'main.postcss';

:root {
  --titleLineHeight: 1.3;
  --fullHeight: 100%;
  --categoryItemMarginBottom: 5px;
  --contentLineHeight: 1.7;
  --contentTopPosition: 2px;
}

.CoveredInClassExport {
  background-color: #525659;
  overflow: hidden;

  &_button {
    position: fixed;
    top: 50%;
  }

  &_frame {
    background-color: #fff;
    box-shadow: 8px 8px 4px black;
    margin: 40px auto;
    padding: 40px 40px 0;
    width: 800px;
  }

  &_section {
    background-color: #fff;
    border-collapse: collapse;
    height: 100%;
    width: 720px;
  }

  &_header {
    background-color: white;
    border: 2px solid #d0d0d0;
    height: 190px;
    margin-bottom: 14px;

    &_companyicon {
      background-image: url(data:image/svg+xml;base64,);
      background-repeat: no-repeat;
      height: 40px;
      width: 68px;
    }
  }

  &_content {
    min-height: 710px;
    overflow: auto;

    &_grid {
      list-style: none;
      margin: 0 0 14px;
    }

    &_category {
      border: 2px solid #d0d0d0;
      font-family: var(--fontFamily);
      height: var(--fullHeight);
      padding: 10px;

      &_title {
        color: var(--colorHintMessage);
        font-size: 16px;
        letter-spacing: var(--letterSpacing);
        line-height: var(--titleLineHeight);
      }

      &_item {
        margin-bottom: var(--categoryItemMarginBottom);

        &_content {
          font-size: 12px;
          letter-spacing: var(--letterSpacing);
          line-height: var(--contentLineHeight);
          position: relative;
          top: var(--contentTopPosition);
        }
      }
    }

    &_activity {
      padding: 10px;

      &_title {
        color: #4a4a4a;
        display: block;
        font-size: 18px;
        font-weight: var(--fontWeight-medium);
        letter-spacing: 1px;
        line-height: 1.7;
        margin: 0 0 10px;
      }
    }
  }

  &_footer {
    background-color: white;
    border: 2px solid #d0d0d0;
    height: 170px;

    &_qrcode {
      background-image: url(data:image/png;base64,);
      background-repeat: no-repeat;
      background-size: 100px;
      float: right;
      height: 100px;
      margin-top: 10px;
      width: 100px;
    }
  }

  & tbody td {
    vertical-align: top;
  }

  &_topcontent {
    background-color: beige;
    border: 1px solid yellowgreen;
    min-height: 250px;
    padding-bottom: 10px;
    padding-top: 10px;
  }

  &_topcontentinfo {
    background-color: teal;
    display: inline-block;
    vertical-align: top;
    width: 49%;
  }

  &_topcontentblank {
    display: inline-block;
    width: 2%;
  }

  &_bottomcontent {
    background-color: bisque;
    border: 1px solid yellowgreen;
    margin-bottom: 20px;
    min-height: 250px;
    padding-bottom: 10px;
    padding-top: 10px;
  }

  &_bottomcontentinfo {
    background-color: burlywood;
    display: inline-block;
    vertical-align: top;
    width: 32%;
  }

  &_bottomcontentblank {
    display: inline-block;
    width: 2%;
  }

  &_blockcontent {
    margin-top: 20px;
  }
}
